{
    "name": "rage-kings/botman-menu-system",
    "license": "MIT",
    "description": "Menu feature for BotMan",
    "keywords": [
        "Bot",
        "BotMan",
        "Menu"
    ],
    "homepage": "https://github.com/rage-kings/botman-menu-system",
    "authors": [
        {
            "name": "Dzianis Abramenka",
            "email": "asmodey2007@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "botman/botman": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BotMan\\Menu\\Unit\\": "tests/Unit",
            "Tests\\BotMan\\Menu\\Fixtures\\": "tests/Fixtures",
            "Tests\\BotMan\\Menu\\Feature\\": "tests/Feature"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    }
}
